#include <stdio.h>
int  fibo(int);
int main()
{
    int a,i;
    printf("Enter a number ");
    scanf("%d",&a);
    printf("Series of fibonicca is:");
    //fibo(a);
     //printf("%d ",x);
     for(i=0;i<a;i++)
     printf("%d ",fibo(i));

    return 0;
}
int fibo(int n)
{
   if((n==1) || (n==0))
    return n;
   return (fibo(n-1)+fibo(n-2));
}
